<header>
    <h1>Centro medico Backend</h1>
    <link rel="short icon" href="estetoscopio.png">
</header>
<main>
    <h2>Listado medicos</h2>
    <table>
        <thead>
            <tr>
                <th>Nombres</th>
                <th>Apellidos</th>
                <th>Número de Cédula</th>
                <th>Consultorio</th>
                <th>Telefono</th>
                <th>Correo</th>
                <th>Especialidad</th>
            </tr>
        </thead>
        <tbody>
            <!-- Se agrega el contenido dinamicamente por medio de Handlebars -->
            {{#each resultado}}
            <tr>
                <td>{{nombres}}</td>
                <td>{{apellidos}}</td>
                <td>{{cedula}}</td>
                <td>{{consultorio}}</td>
                <td>{{telefono}}</td>
                <td>{{correo}}</td>
                <td>{{especialidad}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
    <a href="../index.html"><input type="button" value="Volver"></a>
</main>
<footer>
    <h2>Bienvenidos</h2>
</footer>